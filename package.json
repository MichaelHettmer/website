{
    "name": "website",
    "version": "1.0.1",
    "description": "My portfolio, blog and things I find worse sharing",
    "keywords": [
        "node",
        "app",
        "typescript",
        "gatsby"
    ],
    "homepage": "https://github.com/MichaelHettmer/website#readme",
    "bugs": {
        "url": "https://github.com/MichaelHettmer/website/issues"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/MichaelHettmer/website.git"
    },
    "license": "MIT",
    "author": "MichaelHettmer",
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "files": [
        "dist"
    ],
    "scripts": {
        "build": "gatsby build",
        "build:docker": "gatsby build && docker build -t michaelhettmer/website .",
        "build:serve": "npm run build && gatsby serve",
        "build:watch": "chokidar --initial --verbose \"src/**/*\" -c \"npm run build\"",
        "clean": "gatsby clean",
        "commitlint": "commitlint",
        "commitlint:ci": "commitlint-circle",
        "develop": "gatsby develop",
        "postinstall": "gatsby telemetry --disable",
        "lint": "run-p lint:code lint:styles",
        "lint:ci": "run-p lint:code:ci lint:styles:ci",
        "lint:circleci": "docker run -w /data --rm -v $(pwd):/data circleci/circleci-cli:0.1.6648-alpine config validate",
        "lint:code": "eslint .",
        "lint:code:ci": "mkdir -p reports/eslint && npm run lint:code -- --ext \".js,.jsx,.ts,.tsx,.json,.jsonc\" --format junit --output-file ./reports/eslint/results.xml",
        "lint:code:fix": "npm run lint:code -- --fix",
        "lint:fix": "run-p lint:code:fix lint:styles:fix",
        "lint:styles": "stylelint \"./src/**/*.css\" \"./src/**/*.scss\"",
        "lint:styles:ci": "mkdir -p reports/stylelint && npm run lint:styles -- --custom-formatter \"node_modules/stylelint-junit-formatter\" > ./reports/stylelint/results.xml",
        "lint:styles:fix": "npm run lint:styles -- --fix",
        "serve": "gatsby serve",
        "start": "npm run develop",
        "test": "jest",
        "test:ci": "mkdir -p ./reports/jest && CI=true JEST_JUNIT_OUTPUT_DIR=./reports/jest JEST_JUNIT_OUTPUT_NAME=results.xml npm run test:coverage --forceExit --reporters=\"default\" --reporters=\"jest-junit\"",
        "test:coverage": "npm run test -- --coverage --watchAll=false",
        "typecheck": "tsc --noEmit",
        "validate": "run-p lint typecheck test"
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "dependencies": {
        "@material-ui/core": "4.9.14",
        "@material-ui/icons": "4.5.1",
        "@material-ui/lab": "4.0.0-alpha.56",
        "@svgr/webpack": "5.1.0",
        "axios": "0.19.2",
        "classnames": "2.2.6",
        "gatsby": "2.24.47",
        "gatsby-background-image": "0.9.19",
        "gatsby-image": "2.2.45",
        "gatsby-plugin-manifest": "2.2.48",
        "gatsby-plugin-minify-classnames": "0.1.2",
        "gatsby-plugin-offline": "3.0.42",
        "gatsby-plugin-react-helmet-async": "1.0.16",
        "gatsby-plugin-svgr": "2.0.2",
        "intersection-observer": "0.7.0",
        "lodash": "4.17.19",
        "notistack": "0.9.17",
        "query-string": "6.10.1",
        "react": "16.12.0",
        "react-dom": "16.12.0",
        "react-helmet-async": "1.0.4",
        "react-spring": "8.0.27"
    },
    "devDependencies": {
        "@babel/core": "7.11.1",
        "@commitlint/cli": "8.3.5",
        "@commitlint/config-conventional": "8.3.4",
        "@semantic-release/changelog": "5.0.1",
        "@semantic-release/git": "9.0.0",
        "@testing-library/jest-dom": "5.11.3",
        "@testing-library/react": "9.4.1",
        "@types/classnames": "2.2.10",
        "@types/jest": "25.1.1",
        "@types/lodash": "4.14.159",
        "@types/node": "13.5.0",
        "@types/react": "16.9.46",
        "@types/react-dom": "16.9.8",
        "@typescript-eslint/eslint-plugin": "2.17.0",
        "@typescript-eslint/parser": "2.17.0",
        "babel-eslint": "10.1.0",
        "babel-jest": "25.5.1",
        "babel-plugin-transform-remove-console": "6.9.4",
        "babel-preset-gatsby": "0.2.36",
        "chokidar-cli": "2.1.0",
        "commitlint-circle": "1.0.0",
        "cross-env": "7.0.2",
        "eslint": "6.8.0",
        "eslint-config-prettier": "6.9.0",
        "eslint-config-react-app": "5.1.0",
        "eslint-plugin-flowtype": "4.6.0",
        "eslint-plugin-import": "2.20.0",
        "eslint-plugin-json": "2.0.1",
        "eslint-plugin-jsx-a11y": "6.2.3",
        "eslint-plugin-prettier": "3.1.4",
        "eslint-plugin-react": "7.18.0",
        "eslint-plugin-react-hooks": "2.3.0",
        "gatsby-cli": "2.8.30",
        "gatsby-plugin-postcss": "2.1.22",
        "gatsby-plugin-remove-console": "0.0.2",
        "gatsby-plugin-root-import": "2.0.5",
        "gatsby-plugin-sharp": "2.4.13",
        "gatsby-plugin-typescript": "2.1.27",
        "gatsby-source-filesystem": "2.1.57",
        "gatsby-transformer-json": "2.2.28",
        "gatsby-transformer-sharp": "2.3.19",
        "git-branch-is": "3.1.0",
        "husky": "4.2.5",
        "identity-obj-proxy": "3.0.0",
        "jest": "25.1.0",
        "jest-css-modules-transform": "4.0.0",
        "jest-junit": "10.0.0",
        "jest-mock-console": "1.0.1",
        "lint-staged": "10.0.10",
        "mkdirp": "1.0.3",
        "npm-run-all": "4.1.5",
        "postcss-preset-env": "6.7.0",
        "prettier": "1.19.1",
        "prop-types": "15.7.2",
        "semantic-release": "17.0.8",
        "sharp": "0.24.1",
        "start-server-and-test": "1.10.11",
        "stylelint": "13.0.0",
        "stylelint-config-prettier": "8.0.1",
        "stylelint-junit-formatter": "0.2.2",
        "stylelint-prettier": "1.1.2",
        "trim-right": "1.0.1",
        "ts-jest": "25.0.0",
        "typescript": "3.7.5",
        "typescript-plugin-css-modules": "2.1.2",
        "webpack": "4.41.6"
    },
    "engines": {
        "node": ">=8.0.0"
    }
}
